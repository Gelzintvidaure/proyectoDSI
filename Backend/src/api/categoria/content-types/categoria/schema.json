{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug_categoria": {
      "type": "uid",
      "targetField": "Nombre",
      "required": false
    },
    "estado": {
      "type": "boolean",
      "required": true
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto",
      "mappedBy": "categoria"
    }
  }
}
